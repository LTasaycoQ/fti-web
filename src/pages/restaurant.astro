---
import BaseLayout from "../layouts/BaseLayout.astro"
import Fondos from "../components/fondos.astro"

import RestauranteInicio from "../components/RestaurantInicio.astro"
import GastronomicExperience from "../components/GastronomicExperience.astro"

import fondoImagen from "../assets/backgrounds/interra-intro.webp"
---

<BaseLayout title="Restaurante - Terra Andina Colonial Mansion">

    <Fondos imgFondo={`url(${fondoImagen.src})`} />
    <main>

        <RestauranteInicio />
        <GastronomicExperience />
    </main>

</BaseLayout>

